@phdthesis{Riste2010,
abstract = {Preservation of coherence represents an important step towards the implementa- tion of quantum technologies, where quantum phase is the basic resource. In this thesis, nitrogen-vacancy centers in diamond are used as a testbed for decoupling a qubit from a spin bath. By combining previous knowledge of spin dynamics in NV centers with high-fidelity control of a single qubit, we demonstrate the extension of coherence time of the NV center electron spin up to 26 times, using 136 operations. With double-axis control the decoupling is found to be universal, meaning that all possible states are equally preserved, as it is verified by quantum process tomography. Scaling of coherence time with the number of pulses confirms the model of the spin bath as a slowly fluctuating effective field on the NV center, where a sequence of uniformly spaced pulses is proved to be optimal. Dynamical decoupling is then applied to increase the detection sensitivity in magnetometry, by selecting the ap- propriate number of pulses and their spacing in a sequence. The best measurement conditions are obtained when the sequence is resonant and phase-locked with an oscillating field. Pulse sequences also allow the detection of signals with random phase, showing a dramatic improvement in sensitivity over free evolution of the spin without pulses. Equally spaced pulses provide the highest spectral selectivity and the highest sensitivity to the resonant frequency.},
author = {Rist\`{e}, Diego},
file = {:Users/Adriaan/Library/Application Support/Mendeley Desktop/Downloaded/Rist\`{e} - 2010 - Dynamical decoupling of single spins in diamond.pdf:pdf},
number = {July},
title = {{Dynamical decoupling of single spins in diamond}},
year = {2010}
}
