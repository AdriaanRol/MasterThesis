issn = {1476-1122},
journal = {Nature materials},
keywords = {Carbon Isotopes,Chemical Engineering,Chemical Engineering: methods,Diamond,Diamond: chemistry,Nitrogen,Nitrogen: chemistry,Quantum Theory},
month = may,
number = {5},
pages = {383--7},
pmid = {19349970},
publisher = {Nature Publishing Group},
title = {{Ultralong spin coherence time in isotopically engineered diamond.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19349970},
volume = {8},
