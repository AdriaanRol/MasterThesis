@phdthesis{Rol2011,
abstract = {This thesis investigates the feasibility of using a cross polarizer scheme to sup- press the excitation laser at the detector when using resonant excitation to stimulate NV− centres in diamond. Suppressing the excitation laser is key in achieving measurement based entanglement. The polarization properties of sev- eral optical components are characterized. Most notable is the depolarization at a lens interface, which proves to be the greatest challenge in implementing a cross-polarizer scheme. Finally the data is combined in a model to determine whether the current components provide sufficient suppression. It is concluded that a special lens is required that does not depolarize the laser beam. A suitable objective is suggested.},
author = {M.A.Rol},
