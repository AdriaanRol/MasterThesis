doi = {10.1038/ncomms3017},
file = {:Users/Adriaan/Library/Application Support/Mendeley Desktop/Downloaded/B\"{u}ch et al. - 2013 - Spin readout and addressability of phosphorus-donor clusters in silicon.pdf:pdf},
issn = {2041-1723},
journal = {Nature communications},
