file = {:Users/Adriaan/Dropbox/Studie/TU Delft/Master/Quantum Error Correction Afstudeerproject/Papers and Theses/somepapersthatcontaininformationaboutconcentrationsc/1309.6424v2.pdf:pdf},
issn = {0028-0836},
journal = {Nature},
month = jan,
title = {{Quantum error correction in a solid-state hybrid spin register}},
url = {http://arxiv.org/abs/1309.6424 http://www.nature.com/doifinder/10.1038/nature12919},
year = {2014}
}
